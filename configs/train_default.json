{
    "data_root": "data/flat-design",
    "source_list": "data/flat-design/source_list.json",
    "style_name": "flat",
    "learning_rate": 1e-5,
    "step_milestones": [20, 100],
    "train_epochs": 500,  
    "batch_size": 8,
    "freeze_unet": false,
    "freeze_vae": true,
    "training_timestep_range": [0, 100],
    "num_inference_steps": 100,
    "content_weight": 1e-3,
    "output_dir": "models/flat_all",
    "augmentation": ["double", "retrieval", "synonym"],
    "use_content_loss": false,
    "use_text_inversion": true,
    "use_sparse_update": true,
    "seed": 99999997,
    "pretrained_name": "CompVis/stable-diffusion-v1-4",    
    "image_size": 512,
    "checkpoints": [50, 100, 150, 200, 250, 300, 350, 400, 450, 500],
    "clip_similars": "similars/flat-similars.pt"
}